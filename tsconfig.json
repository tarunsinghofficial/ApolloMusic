{
  "extends": "expo/tsconfig.base",
"compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "ES6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "target": "ESNext",
    "noImplicitAny": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
}
